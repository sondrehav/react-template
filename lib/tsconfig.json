{
  "compilerOptions": {
    "target": "ES2018",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable"],
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "module": "es2022",
    "isolatedModules": true,
    "noEmit": true,
  },
  "include": [
    "./lib",
  ],
}
